{
  "name": "react-42-login",
  "version": "1.0.0",
  "description": "A React Component for 42 Login.",
  "license": "MIT",
  "repository": "mrdotb/react-42-login",
  "author": {
    "name": "mrdotb",
    "email": "baptiste.chaleil@gmail.com",
    "url": "github.com/mrdotb"
  },
  "scripts": {
    "start": "export NODE_ENV=development && node_modules/nodemon/bin/nodemon.js --ignore 'demo/app.js' --ignore 'demo/bundle.js' demo/server.js",
    "bundle": "export NODE_ENV=production && webpack -p --config webpack.config.dist.js",
    "test": "ava",
    "lint": "xo"
  },
  "files": [
    "dist/Login42.js"
  ],
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "42-login",
    "42-oAuth"
  ],
  "dependencies": {
    "object-keys": "^1.0.11",
    "query-string": "^4.2.3",
    "react": "^15.4.1",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "body-parser": "^1.15.2",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-react": "^6.8.0",
    "got": "^6.7.1",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.1",
    "react-json-pretty": "^1.3.1",
    "style-loader": "^0.13.1",
    "webpack": "2.2.0-rc.3",
    "webpack-dev-server": "^1.16.2",
    "xo": "^0.17.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "xo": {
    "esnext": true,
    "extends": "xo-react",
    "envs": [
      "node",
      "browser",
      "es6"
    ],
    "rules": {
      "unicorn/filename-case": 0,
      "react/forbid-component-props": 0,
      "react/no-unused-prop-types": 0
    }
  },
  "ava": {
    "files": [
      "test/test.js"
    ],
    "failFast": true,
    "verbose": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
